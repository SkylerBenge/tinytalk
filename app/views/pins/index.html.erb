
<%= render 'home/signin' %>

<div id ="pins">
<% @pins.each do |pin| %>
  <div class="box">
      <%= image_tag pin.image(:medium) %>
      <p class="description">
      <%= pin.description %>
      </p>
      <p>
        <strong>
          Posted by <%= pin.user.email %>
        </strong>
      </p>
  </div>

<% end %>
</div>